{{# each products}}
<div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="{{this.imagePath}}" class="img-responsive" alt=".">
            <div class="caption">
                <h3>{{this.title}}</h3>
                <p>
                    <a href="#" class="btn btn-primary" role="button">Buy Now</a>
                    <a href="#" class="btn btn-default" role="button">Add to cart</a>
                </p>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{/each}}